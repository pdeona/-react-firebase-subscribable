{
  "name": "react-firebase-subscribable",
  "version": "2.0.1",
  "main": "index.js",
  "author": "Pedro De Ona <pedro@instantmd.co>",
  "license": "MIT",
  "repository": "github:pdeona/-react-firebase-subscribable",
  "description": "Higher Order Components to wrap a component in a firebase subscription",
  "keywords": [
    "firebase",
    "firestore",
    "auth",
    "real time",
    "subscriptions"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist && yarn build-typedecs",
    "build": "yarn babel src -o dist/index.js --extensions \".ts,.tsx\"",
    "deploy": "yarn build && yarn publish",
    "build-typedecs": "yarn tsc -d --outDir dist --emitDeclarationOnly -p ./tsconfig.json",
    "test": "jest --coverage",
    "test:circleci": "jest --ci --reporters=default --reporters=jest-junit --runInBand --json --outputFile=\"reports/jest/results.json\"",
    "test:watch": "jest --watch",
    "ts:watch": "yarn tsc --watch --noEmit -p ./tsconfig.json"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.6",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-helper-evaluate-path": "^0.5.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-preset-minify": "^0.5.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "firebase": "^5.10.1",
    "flow-bin": "^0.86.0",
    "jest": "^24.7.1",
    "jest-pnp-resolver": "^1.0.2",
    "react": "^16.8.6",
    "react-app-polyfill": "^0.1.3",
    "react-dom": "^16.8.6",
    "react-testing-library": "^7.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.3",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript2": "^0.21.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "hoist-non-react-statics": "^3.1.0",
    "rxjs": "^6.5.1",
    "symbol-observable": "^1.2.0"
  },
  "peerDependencies": {
    "firebase": "^5.10.1",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "jest": {
    "reporters": [
      "default"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/(src|tests)/**/*.(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
