{
  "name": "react-firebase-subscribable",
  "version": "1.0.12",
  "source": "src/index.js",
  "main": "lib/index.js",
  "author": "Pedro De Ona <pedro@instantmd.co>",
  "license": "MIT",
  "repository": "github:pdeona/-react-firebase-subscribable",
  "description": "Higher Order Components to wrap a component in a firebase subscription",
  "keywords": [
    "firebase",
    "firestore",
    "auth",
    "real time",
    "subscriptions"
  ],
  "scripts": {
    "build": "yarn build:babel && yarn build:rollup",
    "prebuild": "rimraf lib && rimraf dist",
    "build:babel": "babel src --out-dir lib",
    "build:rollup": "rollup -c",
    "postbuild": "flow-copy-source -v src lib",
    "deploy": "yarn build && yarn publish"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "cpx": "^1.5.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.86.0",
    "flow-copy-source": "^2.0.2",
    "jest": "^23.6.0",
    "jest-pnp-resolver": "^1.0.2",
    "react-app-polyfill": "^0.1.3",
    "react-test-renderer": "^16.6.3",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "hoist-non-react-statics": "^3.1.0",
    "react": "^16.6.0",
    "symbol-observable": "^1.2.0"
  },
  "peerDependencies": {
    "react-dom": "^16.6.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/(src|tests)/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
